{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "module": "esnext",
        "paths": {
            "@/api/*": ["./src/api/*"],
            "@/assets/*": ["./assets/*"],
            "@/auth/*": ["./src/auth/*"],
            "@/type/*": ["./src/type/*"],
            "@/util/*": ["./src/util/*"],
            "@/route/*": ["./src/route/*"],
            "@/screen/*": ["./src/screen/*"],
            "@/context/*": ["./src/context/*"],
            "@/factory/*": ["./src/factory/*"],
            "@/section/*": ["./src/section/*"],
            "@/component/*": ["./src/component/*"],
            "@/controller/*": ["./src/controller/*"]
        }
    }
}
