{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "module": "esnext",
        "paths": {
            "@/auth/*": ["./src/auth/*"],
            "@/type/*": ["./src/type/*"],
            "@/route/*": ["./src/route/*"],
            "@/screen/*": ["./src/screen/*"],
            "@/context/*": ["./src/context/*"],
            "@/component/*": ["./src/component/*"],
        }
    }
}
